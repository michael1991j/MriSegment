#include "MRIDataSet.h"

MRIDataSet::MRIDataSet(vector<string> Filelist , InputType Types) {

  Data = 0;
  QThreadPool *threadPool = QThreadPool::globalInstance();

  this->Sagittal = new vector<Mat>(list->size());
  for(int i=0; i < list->size(); i++)
  {
  DicomtoMat * instance = new DicomtoMat(i,list->at(i).toStdString(),this->sagtical);
  threadPool->start(instance);

  }
  threadPool->waitForDone();

}

